<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:container='urn:import:com.sencha.gxt.widget.core.client.container'
             xmlns:gxt='urn:import:com.sencha.gxt.widget.core.client'
        >

    <ui:style>
        .highlight {
            background-color: #e0ffff;
        }
    </ui:style>

    <ui:with field="westMargins" type="com.sencha.gxt.core.client.util.Margins">
        <ui:attributes top="0" right="5" bottom="5" left="0"/>
    </ui:with>

    <ui:with field="westData"
             type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData">
        <ui:attributes size="100"
                       collapsible="true"
                       split="true"
                       collapseMini="true"
                       margins="{westMargins}"/>
    </ui:with>
    <!--<ui:with field="centerData" type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData"/>-->

    <container:BorderLayoutContainer>
        <!--<container:north>-->
        <!--<g:HTMLPanel>-->
        <!--<p>Hello</p>-->
        <!--</g:HTMLPanel>-->
        <!--</container:north>-->
        <container:west layoutData="{westData}">
            <gxt:ContentPanel/>
        </container:west>
        <container:center>
            <gxt:ContentPanel/>
        </container:center>
    </container:BorderLayoutContainer>
</ui:UiBinder>
