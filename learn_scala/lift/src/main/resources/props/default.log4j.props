#log4j.rootLogger=INFO, AppAppender
log4j.rootCategory=WARN, S

log4j.logger.pkg=INFO
log4j.logger.code=INFO
log4j.logger.bootstrap=INFO

log4j.appender.S=org.apache.log4j.ConsoleAppender
log4j.appender.S.layout=org.apache.log4j.PatternLayout
log4j.appender.S.layout.ConversionPattern=%d{ISO8601} [%t] %-5p %c - %m%n


